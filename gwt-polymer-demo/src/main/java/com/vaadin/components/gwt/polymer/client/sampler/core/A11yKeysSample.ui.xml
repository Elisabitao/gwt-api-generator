<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:p='urn:import:com.vaadin.components.gwt.polymer.client.widget'>
    <ui:style>
        .container {
            font-family: sans-serif;
            margin: 25px 24px;
        }
    </ui:style>

    <g:HTMLPanel addStyleNames="{style.container}">
        <span>Press any of these keys: <br></br><em ui:field="label"><strong>{{keys}}</strong></em></span>
        <p:CoreA11yKeys ui:field="a11y"/>
        <pre ui:field="output"/>
    </g:HTMLPanel>
</ui:UiBinder>
